<article class="store-information app-js__store-information">
    <div class="store-information__info">
        <div class="store-information__info_img">
            <div class="text-with-image__image">
                <div class="store-image">
                    {{> picture
                        desktopSrc = './images/storeInfoImagesdfasdfs.png'
                        mobileSrc  = './images/storeInfoImagesdfsdf.png'
                        isLazyLoad = true
                    }}
                </div>
            </div>
            <div class="store-information__info-buttons">
                {{!-- <button type="button" class="btn--outline">
                    Book Appointment
                </button> --}}
                <button type="button" class="store-information__info_img_link cta" arai-label="Set as my store" data-mipsStoreNumber="30" tabindex="0">
                    <span class="cta-content">
                        <span class="cta-text" tabindex="-1">Set as my store &nbsp; </span>
                    </span>
                    <span class="store-information__info_img_link_icon">
                        <img src="https://media.tiffany.com/is/content/tiffanydmdt3/saved-store_default-1?wid=64&hei=96" altText="${savedIcon.altText}" />
                    </span>
                </button>

                <div class="store-information__stores-nearby hide">
                    <label class="heading">Nearby Stores</label>
                    <a class="nearby cta hide" href="#"
                        data-currentlat="40.7127753"
                        data-currentlang="-74.0059728"
                        data-lat="40.7956049"
                        data-lang="-73.67385480000002">
                        <span class="cta-content">
                            <span class="cta-text" tabindex="-1">New York - Soho </span>
                        </span>
                        <span class="icon-dropdown-right"></span>
                    </a>
                </div>
            </div>
        </div>

        <div class="store-information__info_services">
            <h1 class="heading">New York - Fifth Avenue</h1>
            <p class="store-information__flagship_store">Flagship Store</p>
            <p class="store-information__mips hide">MIPS: #1</p>

            <div class="store-address">
                <a href="http://maps.google.com/maps?q=40.7956049,-73.67385480000002 (Tiffany New York - Fifth Avenue)&amp;mrt=loc&amp;t=m" class="cta-underline cta" tabindex="0">
                    <span class="cta-content">
                        <span class="cta-text" tabindex="-1">727 Fifth Avenue</span>
                        <span class="cta-text" tabindex="-1">New York, NY 10022</span>
                    </span>
                </a>
            </div>


            <p class="store-timings">
                Monday-Saturday  11am-7pm
                <br>
                Sunday  11am-6pm
                <br>
                Sunday  11am-5pm
                <br>
               Motherâ€™s Day Store Hours  11am-6pm
            </p>

            <p class="store-contact">
                <a class="tel-link cta" href="tel:2127558000" tabindex="0">
                    <span class="cta-content">
                        <span class="cta-text" tabindex="-1">(212) 755-8000</span>
                    </span>
                </a><br>
                <a class="email-link cta" href="mailto:email@email.com" tabindex="0">
                    <span class="cta-content">
                        <span class="cta-text" tabindex="-1">Email Store</span>
                    </span>
                </a>
            </p>

            <div class="store-service">
                <p class="heading">Services</p>
                <ul>
                    <li>Diamond engagement ring consultation</li>
                    <li>Jewelry cleaning and repair</li>
                    <li>Jewelry and gift engraving</li>
                    <li>Leather embossing and monogramming</li>
                    <li>Watch repair and sales</li>
                    <li>Glass and crystal etching</li>
                    <li>Personal shopping</li>
                </ul>
            </div>
        </div>
    </div>

    <div class="store-information__map" tabindex="0">
        <tiffany-maps mapcenter="40.7629678" markeratlat="40.7629678" markeratlng="-73.97395670000003" zoomlevel="14"></tiffany-maps>
